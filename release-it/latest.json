{
    "git": {
      "tag": true,
      "tagAnnotation": "Release ${version}",
      "commit": true,
      "commitMessage": "Release v${version}",
      "push": true
    },
    "github": {
      "release": true
    },
    "npm": {
        "publish": false
    },
    "plugins": {
        "@release-it/keep-a-changelog": {
          "filename": "CHANGELOG.md",
          "strictLatest": false,
          "addUnreleased": true,
          "addVersionUrl": true
        }
    }
}