{
    "git": {
      "tag": true,
      "tagAnnotation": "Release ${version}",
      "commit": true,
      "commitMessage": "Release v${version}",
      "push": true,
      "changelog": "npx auto-changelog --stdout"
    },
    "github": {
      "release": true,
      "preRelease": true
    },
    "npm": {
        "publish": false
    },
    "hooks": {
      "after:bump": "npx auto-changelog -p"
    }
}